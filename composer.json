{
    "name": "mrgoodbytes8667/test-discord-faker",
    "type": "library",
    "description": "Discord Faker provider",
    "require": {
        "php": "^8.1",
        "mrgoodbytes8667/test-common-faker": ">=0.3.5"
    },
    "require-dev": {
        "mrgoodbytes8667/discord-response-bundle": "^0.15 | ^0.16",
        "symfony/test-pack": "^1.0.9"
    },
    "authors": [
        {
            "name": "MrGoodBytes",
            "email": "bytes@mrgoodbytes.dev"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "suggest": {
        "mrgoodbytes8667/discord-response-bundle": "Necessary for actual usage",
        "mrgoodbytes8667/test-common": "Common helper traits for PHPUnit with Symfony",
        "symfony/phpunit-bridge": "Integrate Symfony with PHPUnit"
    },
    "conflict": {
        "phpunit/phpunit": "<9.5"
    },
    "autoload": {
        "psr-4": {
            "Bytes\\Common\\Faker\\Providers\\": "Providers/",
            "Bytes\\Common\\Faker\\Discord\\": "src/",
            "Bytes\\Common\\Faker\\Tests\\": "Tests/"
        }
    },
    "minimum-stability": "dev"
}
